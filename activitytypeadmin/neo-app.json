{
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/sap/opu/odata",
      "target": {
        "type": "destination",
        "name": "ERP_PP",
        "entryPath": "/sap/opu/odata"
      },
      "description": "ERP_PP"
    },
    {
      "path": "/resources/NovoTheme",
      "target": {
        "type": "application",
        "name": "NovoTheme",
        "entryPath": "/"
      },
      "description": "This is the Novo Theme"
    },
    {
      "path": "/novo_cat_java_be",
      "target": {
        "type": "destination",
        "name": "javacats"
      },
      "description": "javacats"
    }
  ],
  "sendWelcomeFileRedirect": true
}